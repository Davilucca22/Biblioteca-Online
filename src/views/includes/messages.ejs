<% if(errors) { %>
<div class="row">
    <div class="col">
        <div class="alert alert-danger" style="width: 70%; background-color: rgba(235, 88, 88, 0.452); margin: 0 auto;text-align: center;border-radius: 10px; font-size: 16pt;font-family: sans-serif">
            <% errors.forEach(erro => { %>
                <%= erro %><br>
           <% }) %>
        </div>
    </div>
</div>
<% } %>
